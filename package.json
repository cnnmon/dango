{
  "name": "dango",
  "displayName": "dango",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "enabledApiProposals": [
    "languageModels"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dango.viewFiles",
        "title": "Dango: View Files"
      },
      {
        "command": "dango.addFile",
        "title": "Dango: Add File"
      },
      {
        "command": "dango.find",
        "title": "Dango: Find"
      },
      {
        "command": "dango.naruto",
        "title": "Dango: Naruto"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dango-chat",
          "title": "Dango",
          "icon": "assets/panda.svg"
        }
      ]
    },
    "views": {
      "dango-chat": [
        {
          "id": "dango.chat",
          "type": "webview",
          "name": "Chat"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}
