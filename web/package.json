{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.94",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "dotenv": "^16.4.5",
    "openai": "^4.38.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "source": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --public-url /dist",
    "build": "rm -rf dist/ && parcel build src/index.html && cd dist && for file in $(ls | egrep '\\.(js|css)$'); do if [[ $file =~ ^(.+)\\.[0-9a-f]{8}\\.(js|css)$ ]]; then newname=\"${BASH_REMATCH[1]}.${BASH_REMATCH[2]}\"; echo \"Renaming $file to $newname\"; mv \"$file\" \"$newname\"; fi; done"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "parcel": "^2.12.0",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.3"
  }
}
